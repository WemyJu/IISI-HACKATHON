<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.firsttry.firsttry.MyHistory">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal|top">

        <WebView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/webView2"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:id="@+id/textView30"
            android:layout_gravity="center_horizontal"
            android:layout_weight="2.2" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="2">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:id="@+id/textView27"
                android:layout_gravity="center_horizontal"
                android:layout_weight="4" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/my_history_btn"
                android:layout_gravity="center_horizontal"
                android:layout_weight="2.5"
                android:src="@drawable/my_history" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:id="@+id/textView25"
                android:layout_gravity="center_horizontal"
                android:layout_weight="4.1" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/add_health_info_btn"
                android:layout_gravity="center_horizontal"
                android:layout_weight="2.5"
                android:src="@drawable/add_health_info" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:id="@+id/textView24"
                android:layout_gravity="center_horizontal"
                android:layout_weight="4" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="2" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:id="@+id/textView28"
                android:layout_gravity="center_horizontal"
                android:layout_weight="4" />

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:id="@+id/share_pos_btn"
                android:layout_gravity="center_horizontal"
                android:layout_weight="2.5"
                android:src="@drawable/share_position" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/textView26"
                android:layout_gravity="center_horizontal"
                android:layout_weight="4.1" />

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:id="@+id/weather_repo_btn"
                android:layout_gravity="center_horizontal"
                android:layout_weight="2.5"
                android:src="@drawable/weather_report" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/textView29"
                android:layout_gravity="center_horizontal"
                android:layout_weight="4" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:id="@+id/textView23"
            android:layout_gravity="center_horizontal"
            android:layout_weight="2.2" />

    </LinearLayout>

</FrameLayout>
